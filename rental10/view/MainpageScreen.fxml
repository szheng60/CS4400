<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="rental10.MainpageController">
    <children>
        <Button fx:id="loginButton" layoutX="347.0" layoutY="453.0" onAction="#loginHandler" prefHeight="39.0" prefWidth="124.0" text="Login">
         <font>
            <Font size="18.0" />
         </font></Button>
        <Label fx:id="label" layoutX="126" layoutY="120" minHeight="16" minWidth="69" />
      <GridPane layoutX="150.0" layoutY="240.0" prefHeight="121.0" prefWidth="321.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="176.0" minWidth="10.0" prefWidth="135.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="227.0" minWidth="10.0" prefWidth="227.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label prefHeight="40.0" prefWidth="115.0" text="Username:">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <Label prefHeight="40.0" prefWidth="115.0" text="Password:" GridPane.rowIndex="1">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <TextField id="usernameTextField" fx:id="userName" prefHeight="36.0" prefWidth="96.0" promptText="Enter name" GridPane.columnIndex="1" />
            <PasswordField fx:id="password" prefHeight="36.0" prefWidth="96.0" promptText="Enter Password" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <Label id="titleLabel" alignment="CENTER" prefHeight="100.0" prefWidth="800.0" text="Welcome">
         <font>
            <Font name="Arial Bold" size="48.0" />
         </font>
      </Label>
      <Label fx:id="ProspectiveResidentLabel" layoutX="515.0" layoutY="141.0" onMouseClicked="#newApplicationFormHandler" prefHeight="36.0" prefWidth="253.0" text="Prospective Resident?" textFill="#3c15eb" underline="true" visible="false">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label fx:id="createAccountLabel" layoutX="543.0" layoutY="453.0" onMouseClicked="#registrationFormHandler" prefHeight="36.0" prefWidth="188.0" text="Create Account" textFill="#040017" underline="true">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label id="loginLabel" fx:id="loginLabel" layoutX="265.0" layoutY="368.0" prefHeight="39.0" prefWidth="309.0" textFill="#e30917" wrapText="true">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <Label layoutX="587.0" layoutY="429.0" text="Not A User?">
         <font>
            <Font name="System Italic" size="18.0" />
         </font>
      </Label>
    </children>
    <stylesheets>
        <URL value="@general.css" />
    </stylesheets>
</AnchorPane>
